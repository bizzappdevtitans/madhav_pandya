<?xml version="1.0" ?>
<odoo>
        <!-- Wizard view shown after clicking the menu item of Project -->
        <record id="view_project_creation_order_form" model="ir.ui.view">
            <field name="name">project.wizard.form</field>
            <field name="model">create.project.wizard</field>
            <field name="arch" type="xml">
                <form string="Create Project Through trello">
                    <group>

                    <field name="project_importer" />
                      <button
                        name="import_data_from_trello"
                        type="object"
                        string="Import Project"
                        class="oe_highlight"
                    />
                 <button
                        name="import_task_from_trello"
                        type="object"
                        string="Import stage"
                        class="oe_highlight"
                    />
            </group>
                </form>
            </field>
        </record>
        <!-- Wizard action to the menu button "Project Model" of Project-->
        <record id="action_create_project_wizard" model="ir.actions.act_window">
            <field name="name">Create Project</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">create.project.wizard</field>
            <field name="view_mode">form</field>
            <field name="view_id" ref="view_project_creation_order_form" />
            <field name="target">new</field>
        </record>
</odoo>
